{
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./public",
    "rootDir": "./src",
    "moduleResolution": "node"
  },
  "include": ["src"]
}
